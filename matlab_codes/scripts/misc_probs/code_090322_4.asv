clear; clc; close all;

% Description: Fits the desired curves to the given data

x = [1 1.8 2.3 3.7 5.6 6.5]';
y = [3 5.7 7 15 36 70]';

% Query points
N = 1000;
xq = linspace(x(1),x(end),N);
eqn = 'a*x+b';

% Fit the curve
f = fit(x,y,eqn);

hold on
scatter(x,y)
plot(xq,f(xq))


xlabel('x')
ylabel('Compressive strength [MPa]')
legend('Location','SouthEast')
grid on
